{
    "version": 2,
    "name": "shenma",
    "builds": [
        { "src": ".well-known", "use": "@now/static" }
    ],
    "routes": [
        {
          "src": "/.well-known/(.*)",
          "headers": { "cache-control": "s-maxage=604800" },
          "dest": "/.well-known/$1"
        }
    ]
  }